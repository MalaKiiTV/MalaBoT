# MalaBoT Environment Variables
# Copy this file to .env and fill in your values. DO NOT commit the .env file.

# -- Core Bot Settings --
DISCORD_TOKEN=your_discord_bot_token_here
BOT_PREFIX=!
OWNER_IDS=your_user_id_here,another_user_id_here
DEBUG_GUILDS=your_test_guild_id_here

# -- Database --
DATABASE_URL=sqlite:///data/malabot.db

# -- Optional Logging --
LOG_LEVEL=INFO